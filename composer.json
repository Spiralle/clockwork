{
	"name": "spiralle/clockwork",
	"type": "library",
	"license": "MIT",
	"description": "Clockwork integration for Spiral Framework",
	"require": {
		"php": ">=8.2",
		"itsgoingd/clockwork": "^5.2",
		"spiral/http": "^3.10",
		"nyholm/psr7": "^1.6"
	},
	"autoload": {
		"psr-4": {
			"Spiralle\\Clockwork\\": "src/"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.12",
		"spiral/boot": "^3.14",
		"spiral/monolog-bridge": "^3.14",
		"spiral/cache": "^3.14",
		"spiral/cycle-bridge": "^2.10",
		"spiral/framework": "^3.14"
	},
	"config": {
		"allow-plugins": {
			"spiral/composer-publish-plugin": true
		}
	}
}
